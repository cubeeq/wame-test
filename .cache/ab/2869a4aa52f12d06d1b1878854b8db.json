riginal":{"line":81,"column":0},"generated":{"line":1,"column":522}},{"source":"src/js/components/App.vue","name":null,"original":{"line":81,"column":0},"generated":{"line":1,"column":530}}],"sources":{"src/js/components/App.vue":"<template>\r\n    <main class=\"container\">\r\n\r\n        <!-- Dolpnenie meta dát (MetaData.vue) -->\r\n        <meta-data :product=\"product\"></meta-data>\r\n\r\n        <!-- Názov produktu -->\r\n        <h1 class=\"title\">{{ product.Name }}</h1>\r\n        \r\n        <!-- Popis produktu -->\r\n        <div class=\"row\">\r\n            <div v-if=\"product.Description.length > 100 && hideText === true\">\r\n                <p>{{ product.Description | truncate(100) }}</p>\r\n                <a href=\"#\" @click=\"fullText()\">Read more</a>\r\n            </div>\r\n            <div v-else>\r\n                <p>{{ product.Description }}</p>\r\n            </div>\r\n        </div>\r\n\r\n        <!-- Hodnotenie (StarRating.vue) -->\r\n        <star-rating\r\n            :stars=\"5\"\r\n            :product-id=\"product.ProductId\"\r\n        ></star-rating>\r\n\r\n        <!-- Cenové ralácie -->\r\n        <div class=\"row prices\">\r\n            <span class=\"price\">\r\n                {{ product.Price }}\r\n                {{ giveCurrency() }}\r\n            </span>\r\n            <span class=\"oldPrice\">\r\n                {{ product.OldPrice }}\r\n                {{ giveCurrency() }}\r\n            </span>\r\n            <span class=\"discount\">\r\n                {{ product.Discount }} zľava\r\n            </span>\r\n        </div>\r\n\r\n        <!-- Označenie \"NOVINKA\" -->\r\n        <div class=\"row\">\r\n            <span\r\n                class=\"newTag\"\r\n                v-if=\"product.NewTag === true\"\r\n            >Novinka</span>\r\n        </div>\r\n\r\n        <!-- Zvolaná farba (ColorSelection.vue) -->\r\n        <div class=\"row selected-color\">\r\n            <strong>Farba: </strong> {{ selectedColor | capitalize }}\r\n        </div>\r\n        \r\n        <!-- Voľba farby -->\r\n        <color-selection\r\n            @update-color=\"updateSelectedColor\"\r\n            :colors=\"product.params[1]\"\r\n        ></color-selection>\r\n\r\n        <!-- Input a button  -->\r\n        <div class=\"row columns is-mobile\">\r\n            <input\r\n                class=\"pieces is-rounded\"\r\n                type=\"number\"\r\n                value=\"1\"\r\n                min=\"1\"\r\n                :max=\"product.Quantity\"\r\n            >\r\n            <button class=\"button is-black is-rounded is-fullwidth\">Pridať do košíka</button>\r\n        </div>\r\n\r\n    </main>\r\n</template>\r\n\r\n<script>\r\n\r\n    import axios from 'axios';\r\n    import globalMixin from \"../mixins/globalMixin\";\r\n\r\n    export default {\r\n\r\n        mixins: [globalMixin],\r\n\r\n        data() {\r\n            return {\r\n\r\n                // JSON data\r\n                product: null,\r\n\r\n                // Zvolaná farba z local storage\r\n                selectedColor: localStorage.selectColor || 'Zvoľte farbu',\r\n                \r\n                // Schovanie textu popisu\r\n                hideText: true\r\n\r\n            }\r\n        },\r\n\r\n        mounted() {\r\n            \r\n            /*\r\n            * Pripojenie ku API\r\n            */\r\n            axios\r\n                .get('https://api.jsonbin.io/b/5dbc3e50bf3ef75e90e028a3/latest')\r\n\r\n                .then(response => {\r\n                    this.product = response.data\r\n                })\r\n\r\n                .catch(error => {\r\n                console.log(error);\r\n            });\r\n                \r\n        }\r\n        \r\n    }\r\n\r\n</script>\r\n\r\n<style lang=\"scss\" scoped></style>"},"lineCount":null}},"error":null,"hash":"0a79e7d2be2fc1f4112cc12f65c9ee4a","cacheData":{"env":{}}}